{
  "name": "angular-universal-course",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:prerender": "http-server -c-1 dist/angular-universal-course/browser",
    "dev:ssr": "ng run angular-universal-course:serve-ssr",
    "serve:ssr": "node dist/angular-universal-course/server/main.js",
    "build:ssr": "ng build --prod && ng run angular-universal-course:server:development",
    "prerender": "ng run angular-universal-course:prerender  --routesFile routes.txt",
    "start:heroku": "node dist/server",
    "postinstall": "npm run build:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.3",
    "@angular/cdk": "~10.0.0",
    "@angular/common": "~10.0.3",
    "@angular/compiler": "~10.0.3",
    "@angular/core": "~10.0.3",
    "@angular/forms": "~10.0.3",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~10.0.3",
    "@angular/platform-browser-dynamic": "~10.0.3",
    "@angular/platform-server": "~10.0.3",
    "@angular/router": "~10.0.3",
    "@nguniversal/express-engine": "^10.0.1",
    "express": "^4.15.2",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular/cli": "~10.0.2",
    "@angular/compiler-cli": "~10.0.3",
    "@angular-devkit/build-angular": "~0.1000.2",
    "@nguniversal/builders": "^10.0.1",
    "@types/express": "^4.17.0",
    "@types/node": "^12.11.1",
    "http-server": "^0.12.3",
    "ts-node": "~8.3.0",
    "typescript": "~3.9.5"
  }
}
